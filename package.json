{
  "name": "@emoriiin979/emo-dules",
  "version": "0.3.1",
  "description": "Reusable wrapper classes and utility modules for multiple projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
